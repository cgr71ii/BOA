start: expr
// NUM: /[0-9]+/
NUM: /[0-9]{2,3}/
ADD: "+"
SUB: "-"
MUL: "*"
DIV: "/"
expr: NUM
    | NUM ADD expr
    | NUM SUB expr
    | NUM MUL expr
    | NUM DIV expr

